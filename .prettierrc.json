{
  "singleQuote": true,
  "singleAttributePerLine": true,
  "endOfLine": "crlf",
  "indent_size": 2,
  "printWidth": 80,
  "semi": false,
  "importOrder": ["^@core/(.*)$", "^@server/(.*)$", "^@ui/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["prettier-plugin-tailwindcss"]
}
